@article{neumann14,
	Abstract = {People tend towards wishful thinking, in which they$\backslash$noverestimate the probability of favorable outcomes and$\backslash$nunderestimate the probability of unfavorable outcomes. Many$\backslash$nexplanations for this phenomenon focus on its irrationality. We$\backslash$nexplore whether wishful thinking could actually help people make$\backslash$nbetter decisions given that they have limited cognitive$\backslash$nresources. We consider a situation in which multiple decisions$\backslash$nmust be made over a period of time, where the consequences of$\backslash$nthese decisions are not fully determined. We model this situation$\backslash$nas a Markov decision process, and incorporate limited cognitive$\backslash$nresources by varying the amount of time in the future that the$\backslash$nagent considers the consequences of its decisions. Through$\backslash$nsimulations, we show that with limited cognitive resources, this$\backslash$nmodel can exhibit better performance by incorporating a bias$\backslash$ntowards wishful thinking. This advantage occurs across a range of$\backslash$ndecision-making environments, suggesting that the same effect$\backslash$ncould be applicable to many real life scenarios.},
	Author = {Neumann, Rebecca and Rafferty, Anna N and Griffiths, Thomas L},
	Date-Added = {2016-11-08 22:46:19 +0000},
	Date-Modified = {2016-11-08 22:46:36 +0000},
	File = {:Users/fred/Downloads/WishfulThinking.pdf:pdf},
	Journal = {Proceedings of the 36th Annual Cognitive Science Society Conference},
	Pages = {1210--1218},
	Title = {{A Bounded Rationality Account of Wishful Thinking}},
	Year = {2014},
	Bdsk-File-1 = {}}

@article{muldoon13,
	Abstract = {Information theory is a practical and theoretical framework developed for the study of communication over noisy channels. Its probabilistic basis and capacity to relate statistical structure to function make it ideally suited for studying information flow in the nervous system. As a framework it has a number of useful properties: it provides a general measure sensitive to any relationship, not only linear effects; its quantities have meaningful units which in many cases allow direct comparison between different experiments; and it can be used to study how much information can be gained by observing neural responses in single experimental trials, rather than in averages over multiple trials. A variety of information theoretic quantities are in common use in neuroscience - including the Shannon entropy, Kullback-Leibler divergence, and mutual information. In this entry, we introduce and define these quantities. Further details on how these quantities can be estimated in practice are provided in the entry "Estimation of Information-Theoretic Quantities" and examples of application of these techniques in neuroscience can be found in the entry "Applications of Information-Theoretic Quantities in Neuroscience".},
